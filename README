void bubbleSort() {
	for (int i = 0; i < n - 1; ++i) {
		for (int j = i + 1; j < n; ++j) {
			if (a[i] < a[j]) {
				swap(&a[i], &a[j]);
			}
		}
	}
}

void swap(int *x, int *y) {
	int tmp = *x;
	*x = *y;
	*y = tmp;
}

int max(int a, int b) {
	return a > b ? a : b;
}
